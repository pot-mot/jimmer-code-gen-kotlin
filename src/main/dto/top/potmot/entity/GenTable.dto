GenTableCommonView {
    #allScalars
    id(entity)
}

GenTableColumnsView {
    #allScalars
    id(entity)

    columns {
        #allScalars
        id(table)
        enum {
            id
            name
        }
    }

    indexes {
        #allScalars
        id(table)
        columnIds
    }
}

input GenTableInput {
    #allScalars(this)
    -id
    id(model)
    id(schema)
    id(subGroup)
    id(superTables) as superTableIds

    columns {
        #allScalars(this)
        -id
        id(enum)
    }
}
