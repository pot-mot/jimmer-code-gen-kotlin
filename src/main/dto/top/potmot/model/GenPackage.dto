    #allScalars

    entities {
        #allScalars
    }

    children {
        #allScalars
        entities {
            #allScalars
        }
    }*
}

inputOnly GenPackageInput {
    #allScalars
    -createdTime
    -modifiedTime
}

inputOnly GenPackageTreeInput: GenPackageInput {
    parent {
        #allScalars
        -createdTime
        -modifiedTime
    }*
}
