GenTableGroupCommonView {
    #allScalars
}

GenTableGroupTreeView {
    #allScalars
    tables {
        #allScalars
    }

    children {
        #allScalars
        tables {
            #allScalars
        }
    }*
}

input GenTableGroupCommonInput {
    #allScalars
    -createdTime
    -modifiedTime
}

input GenTableGroupMoveInput {
    id
    id(parent)
}
