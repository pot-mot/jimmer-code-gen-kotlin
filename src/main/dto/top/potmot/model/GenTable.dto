GenTableCommonView {
    #allScalars
    id(schema)
    id(group)
}

GenTableColumnsView {
    #allScalars
    id(schema)
    id(group)

    columns {
        #allScalars
        id(table)
    }
}

input GenTableCommonInput {
    #allScalars
    id(schema)
    id(group)
    -createdTime
    -modifiedTime
}

input GenTableColumnsInput {
    #allScalars
    id(schema)
    id(group)
    -createdTime
    -modifiedTime

    columns {
        #allScalars
    }
}
