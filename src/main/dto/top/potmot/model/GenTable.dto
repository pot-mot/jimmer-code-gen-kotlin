GenTableCommonView {
    #allScalars
    id(schema)
    id(group)
}

GenTableColumnsView {
    #allScalars
    id(schema)
    id(group)

    columns {
        #allScalars
        id(table)
    }
}

inputOnly GenTableCommonInput {
    #allScalars
    id(schema)
    id(group)
    -createdTime
    -modifiedTime
}

inputOnly GenTableColumnsInput: GenTableCommonInput {
    columns {
        #allScalars
    }
}
