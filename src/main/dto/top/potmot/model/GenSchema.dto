GenSchemaView {
    #allScalars
    id(dataSource)
}

GenSchemaPreview {
    #allScalars(GenSchema)
    -id
    id(dataSource)
}

input GenSchemaInput {
    #allScalars(GenSchema)
    -id
    dataSourceId

    tables {
        #allScalars(GenTable)
        -id
        id(model)

        columns {
            #allScalars(GenColumn)
            -id
        }
    }
}
